<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <mimeMap fileExtension=".less" mimeType="text/css" />
        </staticContent>
        <rewrite>
            <rules>
                <rule name="Don't rewrite existing resources" stopProcessing="true">
                    <!--match url="^.*$" /-->
                    <conditions logicalGrouping="MatchAny">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" />
                        <add input="{REQUEST_FILENAME}" matchType="IsDirectory" />
                    </conditions>
                    <action type="None" />
                </rule>
                <rule name="Rewrite to _index.php" stopProcessing="true">
                    <!--match url="^.*$" /-->
                    <action type="Rewrite" url="_index.php" />
                </rule>
            </rules>
        </rewrite>
        <defaultDocument>
            <files>
                <add value="_index.php" />
            </files>
        </defaultDocument>
    </system.webServer>
</configuration>